{
    "name": "OCC Apps Api Dev",
    "dockerComposeFile": [
        "../../infra/compose/compose.network.yml",
        "../../infra/compose/compose.api.dev.yml",
        "../../infra/compose/compose.localstack.yml"
    ],
    "service": "api",
    "workspaceFolder": "/workspace",
    "features": {
        "ghcr.io/devcontainers/features/terraform:1": {},
        "ghcr.io/devcontainers/features/aws-cli:1": {}
    },
    "customizations": {
        "vscode": {
            "settings": {
                "go.useLanguageServer": true,
                "gopls": {
                    "ui.semanticTokens": true
                }
            },
            "extensions": [
                "golang.Go",
                "ms-azuretools.vscode-docker",
                "hashicorp.terraform"
            ]
        }
    },
    "remoteUser": "root"
}